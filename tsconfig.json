{
    "compilerOptions": {
      "sourceMap": true,
      "noImplicitAny": false,
      "module": "Esnext",
      "target": "es5",
      "jsx": "react-jsx",
      "allowJs": true,
      "moduleResolution": "node",
      "outDir": "./dist",
      "rootDir": ".",
      "baseUrl": ".",
      "allowSyntheticDefaultImports":true,
      "lib": ["es5", "es6", "dom"],
      "paths":{
        "@/*": ["src/*"]
      }
    },
    "include": ["src"]
  }
  
    